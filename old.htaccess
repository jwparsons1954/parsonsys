RewriteEngine on

RewriteCond %{HTTP_REFERER} !^$
RewriteCond %{HTTP_REFERER} !^http://parsonsys.net/.*$      [NC]
RewriteCond %{HTTP_REFERER} !^http://parsonsys.net$      [NC]
RewriteCond %{HTTP_REFERER} !^http://www.parsonsys.net/.*$      [NC]
RewriteCond %{HTTP_REFERER} !^http://www.parsonsys.net$      [NC]
RewriteCond %{HTTP_REFERER} !^https://parsonsys.net/.*$      [NC]
RewriteCond %{HTTP_REFERER} !^https://parsonsys.net$      [NC]
RewriteCond %{HTTP_REFERER} !^https://www.parsonsys.net/.*$      [NC]
RewriteCond %{HTTP_REFERER} !^https://www.parsonsys.net$      [NC]
RewriteRule .*\.(jpg|jpeg|gif|png|bmp|txt|htm|html|php)$ - [F,NC]

